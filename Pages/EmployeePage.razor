@page "/employeepage"

<h3>Employees List</h3>

<table class="table table-striped">
    <thead>
        <tr>
            <th>EmployeeId</th>
            <th>Another Info</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var emp in Employees)
        {
            <tr>
                <td>@emp.EmployeeId</td>
                <td><a href="#"class="btn btn-info"></a></td>
            </tr>
        }
    </tbody>
</table>